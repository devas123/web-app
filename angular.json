{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "compmanager-frontend": {
      "projectType": "application",
      "root": "packages/compmanager-frontend",
      "sourceRoot": "packages/compmanager-frontend/src",
      "prefix": "compmanager-frontend",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "../dist",
            "preserveSymlinks": true,
            "allowedCommonJsDependencies": [
              "lodash",
              "uuid/v4",
              "sockjs-client",
              "is-ua-webview",
              "marked"
            ],
            "sourceMap": {
              "scripts": true,
              "styles": true,
              "vendor": true
            },
            "index": "packages/compmanager-frontend/src/index.html",
            "main": "packages/compmanager-frontend/src/main.ts",
            "polyfills": "packages/compmanager-frontend/src/polyfills.ts",
            "tsConfig": "packages/compmanager-frontend/tsconfig.app.json",
            "aot": true,
            "assets": [
              "packages/compmanager-frontend/src/favicon.ico",
              "packages/compmanager-frontend/src/assets"
            ],
            "styles": [
              "tools/fui/semantic/dist/semantic.min.css",
              "packages/compmanager-frontend/src/styles.scss",
              "node_modules/font-awesome/css/font-awesome.css"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "packages/compmanager-frontend/src/environments/environment.ts",
                  "with": "packages/compmanager-frontend/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "compmanager-frontend:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "compmanager-frontend:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "compmanager-frontend:build"
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": [
              "packages/compmanager-frontend/**/*.ts",
              "packages/compmanager-frontend/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:jest",
          "options": {
            "jestConfig": "packages/compmanager-frontend/jest.config.js",
            "tsConfig": "packages/compmanager-frontend/tsconfig.spec.json",
            "passWithNoTests": true,
            "setupFile": "packages/compmanager-frontend/src/test-setup.ts"
          }
        }
      }
    },
    "ng2-semantic-ui": {
      "projectType": "library",
      "root": "packages/ng2-semantic-ui",
      "sourceRoot": "packages/ng2-semantic-ui/src",
      "prefix": "frontend-nx",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "packages/ng2-semantic-ui/tsconfig.lib.json",
            "project": "packages/ng2-semantic-ui/ng-package.json"
          },
          "configurations": {
            "production": {
              "project": "packages/ng2-semantic-ui/ng-package.prod.json",
              "tsConfig": "packages/ng2-semantic-ui/tsconfig.lib.prod.json"
            }
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": [
              "packages/ng2-semantic-ui/**/*.ts",
              "packages/ng2-semantic-ui/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:jest",
          "options": {
            "jestConfig": "packages/ng2-semantic-ui/jest.config.js",
            "tsConfig": "packages/ng2-semantic-ui/tsconfig.spec.json",
            "passWithNoTests": true,
            "setupFile": "packages/ng2-semantic-ui/src/test-setup.ts"
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@angular-eslint/schematics"
  }
}
