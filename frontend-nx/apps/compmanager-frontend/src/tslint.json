{
  "extends": "../tslint.json",
  "rules": {
    "no-bitwise": false,
    "directive-selector": [
      true,
      "attribute",
      "app",
      "camelCase"
    ],
    "component-selector": [
      true,
      "element",
      "app",
      "kebab-case"
    ],
    "semicolon": [
      true,
      "always",
      "ignore-bound-class-methods"
    ]
  }
}
