{
  "name": "frontend-nx",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "nx": "nx",
    "start": "ng serve compmanager-frontend --proxy-config proxy.conf.js",
    "start-aot": "ng serve compmanager-frontend --proxy-config proxy.conf.js --aot",
    "start-sandbox": "ng serve sandbox --proxy-config proxy.conf.js --aot",
    "build-prod": "ng build --prod",
    "build": "ng build",
    "test": "ng test",
    "lint": "nx workspace-lint && ng lint",
    "e2e": "ng e2e",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "update": "ng update @nrwl/workspace",
    "workspace-schematic": "nx workspace-schematic",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "postinstall": "ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points",
    "lib:lint": "ng lint ng2-semantic-ui",
    "lib:test": "ng test ng2-semantic-ui --code-coverage --source-map false --progress false --watch false",
    "lib:compile:locales": "tsc --project src/behaviors/localization/tsconfig.locales.json && node ./scripts/locales",
    "lib:compile": "ng build ng2-semantic-ui && npm run lib:compile:locales",
    "lib:compile:w": "ng build ng2-semantic-ui --watch",
    "lib:compile:prod": "ng build ng2-semantic-ui --prod && npm run lib:compile:locales",
    "lib:build": "npm run lib:lint && npm run lib:compile",
    "lib:build:prod": "npm run lib:lint && npm run lib:compile:prod",
    "build:prod": "npm run lib:build:prod && npm run demo:build:prod"
  },
  "private": true,
  "dependencies": {
