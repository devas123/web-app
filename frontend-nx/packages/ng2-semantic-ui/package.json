{
  "name": "@frontend-nx/ng2-semantic-ui",
  "version": "0.0.1",
  "scripts": {
    "lib:lint": "ng lint ng2-semantic-ui",
    "lib:test": "ng test ng2-semantic-ui --code-coverage --source-map false --progress false --watch false",
    "lib:compile": "ng build ng2-semantic-ui",
    "lib:compile:w": "ng build ng2-semantic-ui --watch",
    "lib:compile:prod": "ng build ng2-semantic-ui --prod",
    "lib:build": "yarn run lib:lint && yarn run lib:compile",
    "lib:build:prod": "yarn run lib:lint && yarn run lib:compile:prod",
    "build:prod": "yarn run lib:compile",
    "build": "yarn run build:prod"
  },
  "peerDependencies": {
    "@angular/common": "^12.2.8",
    "@angular/compiler": "^12.2.8",
    "@angular/core": "^12.2.8",
    "@angular/forms": "^12.2.8",
    "date-fns": "^2.24.0",
    "rxjs": "^7.3.0"
  }
}
